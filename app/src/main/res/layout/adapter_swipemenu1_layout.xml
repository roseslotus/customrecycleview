<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="C测试"
        android:gravity="center"
        android:padding="10dp"
        />

    <com.lotus.recyclerlibrary.recycleview.swipemenu.SuperSwipeMenuLayout
        android:id="@+id/swipe_listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include
            android:id="@+id/smContentView"
            layout="@layout/item_swipe_menu_content" />

        <include
            android:id="@+id/smMenuView"
            layout="@layout/item_swipe_menu1" />

    </com.lotus.recyclerlibrary.recycleview.swipemenu.SuperSwipeMenuLayout>
</LinearLayout>

